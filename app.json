{
  "expo": {
    "name": "gym-plus",
    "slug": "gym-plus",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "gymplus",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "foregroundImage": "./assets/images/logo-text.png",
      "resizeMode": "contain",
      "backgroundColor": "#1F5F8A"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/logo-text.png",
        "backgroundColor": "#1F5F8A"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.lucas.codex.gymplus"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": ["expo-router", "expo-font", "expo-localization"],
    "experiments": {
      "typedRoutes": true
    },
    "packagerOpts": {
      "sourceExts": ["js", "jsx", "svgx", "ts", "tsx"],
      "transformer": "node_modules/react-native-svg-transformer/index.js"
    }
  }
}
